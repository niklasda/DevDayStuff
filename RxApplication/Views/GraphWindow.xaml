<Window x:Class="RxApplication.Views.GraphWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RxApplication.Views"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="GraphWindow" Height="300" Width="300" Icon="/RxApplication;component/Icon/Rx_Icon.png">
    <Grid DataContext="{Binding Source={StaticResource Locator}, Path=Graph}">
        <oxy:PlotView Model="{Binding GraphData}"/>
    </Grid>
</Window>
